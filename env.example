# MyAnimeList OAuth App Credentials
# Get these from https://myanimelist.net/apiconfig
MAL_CLIENT_ID=your_client_id_here
MAL_CLIENT_SECRET=your_client_secret_here

# Redirect URI - update this to your Vercel domain after deployment
MAL_REDIRECT_URI=https://your-app.vercel.app/callback

# Optional: PKCE method (S256 or PLAIN)
MAL_PKCE_METHOD=PLAIN
